{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug React App",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["start", "--", "--inspect-brk"],
        "port": 9229,
        "protocol": "inspector",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  }
  